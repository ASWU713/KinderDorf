<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/requestDateData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Date Request Data"
        android:textAlignment="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/priceData">
    </TextView>
    <TextView
        android:id="@+id/priceData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Price Data"
        android:textAlignment="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/requestDateData"
        app:layout_constraintEnd_toStartOf="@id/sellerData">
    </TextView>
    <TextView
        android:id="@+id/sellerData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Seller Data"
        android:textAlignment="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/priceData"
        app:layout_constraintEnd_toStartOf="@id/buyerData">
    </TextView>
    <TextView
        android:id="@+id/buyerData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Buyer Data"
        android:textAlignment="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/sellerData"
        app:layout_constraintEnd_toEndOf="parent">
    </TextView>


</androidx.constraintlayout.widget.ConstraintLayout>